<thead>	<tr>		#if($isInChargeof)		<th><label title="客户经理">客户经理</label></th>		#end		<th><label title="产品类型">产品类型</label></th>		<th><label title="产品编号">产品编号</label></th>		<th><label title="产品名称">产品名称</label></th>		<th><label title="拟营销额(万元)">拟营销额(万元)</label></th>		<th><label title="营销备注">营销备注</label></th>		<th><label title="实际销售额(万元)">实际销售额(万元)</label></th>	</tr></thead><tbody>	#foreach($data in $!dataList)	<tr>		#if($isInChargeof)		<td><label title="$!data.executeUserName">$!data.executeUserName</label></td>		#end		<td>			<label title="$!data.templateName">$!data.templateName</label>			#if ($userId == $data.executeUserId)			<input type="hidden" name="planDetailId" value="$!data.planDetailId" />			#end		</td>		<td><label title="$!data.productCode">$!data.productCode</label></td>		<td><label onclick="detailProduct($!data.productId)" class="ui-link-btn" title="$!data.productName" id="pdt_p_$!data.planDetailId">$!data.productName</label>			<input type="hidden" id="pName_$!data.productId" value="$!data.productName"/>		</td>		<td>			#if ($userId == $data.executeUserId)			<div class="ui-div-text Myedit" id="$!data.planDetailId">				<input type="text" class="text" id="p_$!data.planDetailId" fun="vTarget" name="planDetailTarget" maxlength="10" value="$vmHelper.getDecimalMoney($!data.planDetailTarget)"/>				<input type="hidden" class="text" id="hd_$!data.planDetailId" value="$vmHelper.getDecimalMoney($!data.planDetailTarget)"/>			</div>			#else			<label title="$vmHelper.getDecimalMoney($!data.planDetailTarget)">$vmHelper.getDecimalMoney($!data.planDetailTarget)</label>			#end			<div class="MyeditInfo" style="display:none;" id="tips_$!data.planDetailId">				<table cellpadding="0" cellspacing="0">					<tbody>						<tr>							<td colspan="2">								<h3>产品指标：<label class="wote" style="width:200px;" title="$!data.productTarget">$!data.productTarget</label></h3>							</td>						</tr>						<tr>							<td colspan="2">								<p>日均剩余营销额：<label>#if($!data.daysRemainTarget<0)0.0#else$vmHelper.getDecimalWanMoney($!data.daysRemainTarget)#end 万元</label></p>								<input type="hidden" id="days_p_$!data.planDetailId" value="$!data.daysRemainTarget" />								<input type="hidden" id="rate_p_$!data.planDetailId" value="$!data.templateRate" />							</td>						</tr>						<tr>							<td>								<p>营销目标：<label class="wote" style="width:60px;" title="$vmHelper.getDecimalWanMoney($!data.marketingTarget)">$vmHelper.getDecimalWanMoney($!data.marketingTarget)</label></p>								<p>已销售额：<label class="wote" style="width:60px;" title="$vmHelper.getDecimalWanMoney($!data.saleMoney)">$vmHelper.getDecimalWanMoney($!data.saleMoney)</label></p>							</td>							<td>								<h4>差额：<label class="wote" style="width:60px;" title="$vmHelper.getDecimalWanMoney($!data.remainTarget)">$vmHelper.getDecimalWanMoney($!data.remainTarget)</label></h4>							</td>						</tr>						<tr>							<td colspan="2">								<h4>拟营销额：<label>$vmHelper.getDecimalWanMoney($!data.planDetailTarget)</label></h4>								#set($target=$!data.remainTarget - $!data.planDetailTarget)								<h4>未计划营销额：#if($!data.notPlanedTarget > 0)<label>$vmHelper.getDecimalWanMoney($target)</label>#end</h4>							</td>						</tr>					</tbody>				</table>			</div>		</td>		<td #if ($userId == $data.executeUserId) class="edit" #end >			<label title="$!subString.replaceRemark($!data.remark)">$!data.remark</label>			#if ($userId == $data.executeUserId)			<input type="hidden" name="remark" value="$!data.remark" />			#end		</td>		<td><label title="$!data.todaySaleMoney" id="t_p_$!data.planDetailId">$vmHelper.getDecimalWanMoney($!data.todaySaleMoney)</label></td>	</tr>	#end</tbody>