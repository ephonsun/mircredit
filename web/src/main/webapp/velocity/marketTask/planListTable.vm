<thead>	<tr>		<th><label title="计划日期">计划日期</label><input type="hidden" id="productCount" value="$!page.totalRowsAmount" /></th>		<th><label title="执行对象">执行对象</label></th>		<th><label title="计划营销额(万元)">计划营销额(万元)</label></th>		<th><label title="实际营销额(万元)">实际营销额(万元)</label></th>		<th><label title="操作">操作</label></th>	</tr></thead><tbody>	#foreach($data in $!dataList)	<tr>		<td><label title="$date.format("yyyy-MM-dd",$!data.planDate)">$date.format("yyyy-MM-dd",$!data.planDate)</label></td>		<td><label title="$!data.executeUserName">$!data.executeUserName</label></td>		<td><label title="$vmHelper.getDecimalWanMoney($!data.planTotalTarget)">$vmHelper.getDecimalWanMoney($!data.planTotalTarget)</label></td>		<td><label title="$vmHelper.getDecimalWanMoney($!data.saleTotalTarget)">$vmHelper.getDecimalWanMoney($!data.saleTotalTarget)</label></td>		<td><label onclick="toDetailList($!data.planId)" class="ui-link-btn" title="查看营销计划">查看营销计划</label></td>	</tr>   #end</tbody><tfoot>	<tr>		<td colspan="5">		   <div class="pagination">				<input type="hidden" id="userIdsSearch" value="$!selectedUserIds"/>				<input type="hidden" id="planDateFromSearch" value='$!date.format("yyyy-MM-dd",$!planDateFrom)'/>				<input type="hidden" id="planDateToSearch" value='$!date.format("yyyy-MM-dd",$!planDateTo)'/>				#parse("/velocity/ajaxPage.vm")			</div>		</td>	</tr></tfoot>