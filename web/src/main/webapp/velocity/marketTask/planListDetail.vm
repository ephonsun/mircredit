#parse("/velocity/taglibs.vm")<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><head>    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />    <title>产品营销计划表</title>    	<link type="text/css" rel="stylesheet" href="${ctx}/css/basic.css" />    <link type="text/css" rel="stylesheet" href="${ctx}/css/public.css" />    <style type="text/css">		div.mt { margin-top: 0 !important; }		span.ml { margin-left: -1px; }	</style>    #scriptLink("core")    <script type="text/javascript" src="${ctx}/js/system/b.base.js"></script>	<script type="text/javascript" src="${ctx}/js/controls/jquery.table-v1.0.js"></script>    <script type="text/javascript">    	$(function(){			$('table.datatbl').table();						/** 取消按钮 **/			$('.ui-footer-btn1-cancel').click(function(){				window.close();			});		});		    </script></head><body><div style="margin:10px;">	<div class="center" style="margin-bottom:5px;">		<span class="bold">			<label>$!userName</label>			<label>$date.format("yyyy-MM-dd",$planDate)</label>			<label>产品营销计划表</label>		</span>	</div><table class="datatbl">	<thead>		<tr>			<th><label title="产品编号">产品编号</label></th>			<th><label title="产品名称">产品名称</label></th>			<th><label title="拟营销额(万元)">拟营销额(万元)</label></th>			<th><label title="已销售额(万元)">已销售额(万元)</label></th>			<th><label title="营销备注">营销备注</label></th>		</tr>	</thead>	<tbody>		#foreach($data in $!dataList)	   <tr>			<td><label title="$!data.productCode">$!data.productCode</label></td>			<td><label title="$!data.productName">$!data.productName</label></td>			<td><label title="$vmHelper.getDecimalWanMoney($!data.planDetailTarget)">$vmHelper.getDecimalWanMoney($!data.planDetailTarget)</label></td>			<td><label title="$vmHelper.getDecimalWanMoney($!data.todaySaleMoney)">$vmHelper.getDecimalWanMoney($!data.todaySaleMoney)</label></td>			<td><label title="$!subString.replaceRemark($!data.remark)">$!data.remark</label></td>		</tr>		#end	</tbody></table><hr /><div class="center">	<input type="button" class="ui-footer-btn1 ui-footer-btn1-cancel" value="关闭" /></div></div></body></html>