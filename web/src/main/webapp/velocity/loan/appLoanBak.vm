<script >
    $(function(){

        $("#cusMarriageBak").change(function() {
            var selectId = $("#cusMarriageBak").find("option:selected").val();
            var appLoanTypeId = $("#appLoanTypeIdBak").find("option:selected").val();
            if (selectId == "02") {
                $(".isMarriageBak").css("display", "");
                if(appLoanTypeId == "2"){
                    $(".cusMateCompanyBak").css("display","");
                }
            } else {// 未婚或者没有选择
                $(".isMarriageBak").css("display", "none");
                $(".cusMateCompanyBak").css("display","none");
            }

        });

        $("#bizIsBelongMybank").change(function() {

            var selectId = $("#bizIsBelongMybank").find("option:selected").val();
            if (selectId == "是") {
                $(".myBankBak").css("display", "");
            } else {
                $(".myBankBak").css("display", "none");
            }

        });

        $("#bizOrgIdBak").change(function(){

            var selectId = $("#bizOrgIdBak").find("option:selected").val();
            if (selectId == "02") {
                $(".bizOrgIdBak").css("display", "");
            } else {
                $(".bizOrgIdBak").css("display", "none");
                $(".bizSocialCreditNumBak").css("display", "none");
                $(".bizOrgCodeBak").css("display", "none");
            }
        });

        $("#gtgshBak").change(function(){

            var selectId = $("#gtgshBak").find("option:selected").val();
            if (selectId == "01") {
                $(".bizOrgCodeBak").css("display", "");
                $(".bizSocialCreditNumBak").css("display", "none");
            } else if(selectId == "02") {
                $(".bizSocialCreditNumBak").css("display", "");
                $(".bizOrgCodeBak").css("display", "none");
            }else{
                $(".bizSocialCreditNumBak").css("display", "none");
                $(".bizOrgCodeBak").css("display", "none");
            }
        });


        $("#appGuarantorWayIdBak").change(function() {

            var selectId = $("#appGuarantorWayIdBak").find("option:selected").val();
            if (selectId == "02") {// 质押
                $(".zyBak").css("display", "");
            } else {
                $(".zyBak").css("display", "none");
                $(".zyOtherBak").css("display", "none");
            }

            if (selectId == "01") {// 抵押
                $(".dyBak").css("display", "");
            } else {
                $(".dyBak").css("display", "none");
                $(".dyOtherBak").css("display", "none");
            }

        });

        $("#appMortgageIdBak").change(function() {
            var selectId = $("#appMortgageIdBak").find("option:selected").val();
            if (selectId == "06") {// 质押其他
                $(".zyOtherBak").css("display", "");
            } else {
                $(".zyOtherBak").css("display", "none");
            }
        });

        $("#appPledgeIdBak").change(function() {
            var selectId = $("#appPledgeIdBak").find("option:selected").val();
            if (selectId == "07") {// 抵押其他
                $(".dyOtherBak").css("display", "");
            } else {
                $(".dyOtherBak").css("display", "none");
            }
        });


        $("input[name='registerInfoSourceIdBak']")
                .change(
                function() {
                    var selectId = $("input:checkbox[name='registerInfoSourceIdBak']:checked");
                    var registerInfoSourceId = "";
                    var directMarket = 0;
                    var introduct = 0;
                    var sourceOther = 0;
                    var i = 0;
                    selectId.each(function() {
                        if ($(this).val() == "1") {
                            $("#registerDirectMarketBak").show();
                            directMarket = 1;
                        }

                        if ($(this).val() == "4") {
                            $("#registerExistCusIntroductBak").show();
                            introduct = 1
                        }

                        if ($(this).val() == "12") {
                            $("#registerInfoSourceOtherBak").show();
                            sourceOther = 1
                        }

                        if (i == 0) {
                            registerInfoSourceId = $(this).val();
                            i = 1;
                        } else {
                            registerInfoSourceId += "," + $(this).val();
                        }

                    });

                    if (directMarket == 0) {
                        $("#registerDirectMarketBak").val("");
                        $("#registerDirectMarketBak").hide();
                    }

                    if (introduct == 0) {
                        $("#registerExistCusIntroductBak").val("");
                        $("#registerExistCusIntroductBak").hide();
                    }

                    if (sourceOther == 0) {
                        $("#registerInfoSourceOtherBak").val("");
                        $("#registerInfoSourceOtherBak").hide();
                    }

                });

        $("input[name='registerInfoSourceIdBak']").change();
        $("#cusMarriageBak").change();
        $("#appGuarantorWayIdBak").change();
        $("#appMortgageIdBak").change();
        $("#appPledgeIdBak").change();
        $("#bizOrgIdBak").change();
        $("#gtgshBak").change();
    });

    //打印
    jQuery('#print1').click(function () {
        var oper = 1;
        preview(oper);
    });



    function preview(oper) {
        if (oper < 10){
            bdhtml=window.document.body.innerHTML;//获取当前页的html代码
            sprnstr="<!--startprint"+oper+"-->";//设置打印开始区域
            eprnstr="<!--endprint"+oper+"-->";//设置打印结束区域
            prnhtml=bdhtml.substring(bdhtml.indexOf(sprnstr)+18); //从开始代码向后取html

            prnhtml=prnhtml.substring(0,prnhtml.indexOf(eprnstr));//从结束代码向前取html
            window.document.body.innerHTML=prnhtml;
            window.print();
            document.location.reload(true)

        }
        else{
            window.print();
        }

    }


</script>
