var setting = {
    data: {
        simpleData: {
            enable: true
        }
    },
    callback: {
        onClick: onClick
    },
    view: {
        dblClickExpand: false,
        showIcon: false
    }

};
var zNodes = $!deptJson;
function onClick(e, treeId, treeNode) {
    var zTree = jQuery.fn.zTree.getZTreeObj("deptTree"),
            nodes = zTree.getSelectedNodes(),
            v = "";
    nodes.sort(function compare(a, b) {
        return a.id - b.id;
    });
    for (var i = 0, l = nodes.length; i < l; i++) {
        v += nodes[i].name + ",";
    }
    if (v.length > 0) v = v.substring(0, v.length - 1);
    var obj = $("#registerRecommendBank");
    obj.attr("value", v);
    $("#deptId").attr("value", treeNode.id);
}
function showMenu() {
    var cityObj = $("#registerRecommendBank");
    var cityOffset = $("#registerRecommendBank").offset();
    $("#menuContent").css({left: cityOffset.left + "px", top: cityOffset.top + cityObj.outerHeight() + "px"}).slideDown("fast");
    $("body").bind("mousedown", onBodyDown);
}
function hideMenu() {
    $("#menuContent").fadeOut("fast");
    $("body").unbind("mousedown", onBodyDown);
}
function onBodyDown(event) {
    if (!(event.target.id == "menuBtn" || event.target.id == "registerRecommendBank" || event.target.id == "menuContent" || $(event.target).parents("#menuContent").length > 0)) {
        hideMenu();
    }
}
var settingData = {
    check: {
        enable: true,
        chkStyle: "checkbox",
        chkboxType: { "Y": "s", "N": "s" }
    },
    data: {
        simpleData: {
            enable: true
        }
    },
    callback: {
        onCheck: onAuthClick,
        beforeCheck: beforeAuthcheck
    },
    view: {
        showIcon: false
    }
};
var zDataNodes = $!businessJsonArray;
//业务主管树选中
function onAuthClick(e, treeId, treenode) {
    var tree = jQuery.fn.zTree.getZTreeObj("dataAuthTree"),
            nodes = treenode.children;
    var bool = treenode.checked;
    if (bool && typeof(nodes) != 'undefined') {
        for (var i = 0; i < nodes.length; i++) {
            tree.setChkDisabled(nodes[i], bool);
        }
    }
    var vid = "";
    var d = tree.getCheckedNodes(), v = '';
    jQuery.each(d, function (i) {
        if (!d[i].chkDisabled) {
            v += d[i].name + ',';
            vid += d[i].id + ',';
        }
    });
    if (v.length > 0) v = v.substring(0, v.length - 1);
    var cityObj = $("#deptRange");
    cityObj.attr("value", v);
    $("#bussdept").attr("value", vid);
}
function beforeAuthcheck(treeid, treenode) {
    var tree = jQuery.fn.zTree.getZTreeObj("dataAuthTree"),
            nodes = treenode.children;
    var bool = treenode.checked;
    if (bool && typeof(nodes) != 'undefined') {
        for (var i = 0; i < nodes.length; i++) {
            tree.setChkDisabled(nodes[i], !bool);
        }
    }
}

