function clickCus(node){

    //验证客户为不良客户
    var isNogoodCus = isNogood(customerId);
    if(isNogoodCus){
        //验证不通过，不可以重新申请贷款
        var msg = "客户“"+customerName+"”为系统不良客户，不允许申请贷款！";
        banger.page.showMessageBox(msg);
        jQuery('#txtCustomerName').val("");
        jQuery("#txtCphNumber").val("");
        jQuery("#txtIDCard").val("");
        return;
    }

    //取消必填验证
