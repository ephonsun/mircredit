    jQuery("#cusMobilePhone").val(node['cphNumber']);
    jQuery("#cusIdcard").val(node['idCard']);
	
	$("#cusIdtypeId option[value='"+node['credentialTypeId']+"']").attr("selected", "selected");
	$("#cusIdtypeId").select();
	showBirthday(node['idCard']);
}

//验证申请贷款数据是否有效
function vLoanData(){
    var isOk = saveBaseInfo(1);
    if (!isOk) {
        return false;
    }

    var customerId = $("#txtCustomerId").val();
    var customerName = $("#txtOldCustomerName").val();
    if(customerId != ""){
        var postJson = {};
        postJson['customerId'] = customerId;
        postJson['customerName'] = customerName;
        var urlStr = "${ctx}/loan/vLoanData.html?random="+Math.random()*1000;
        jQuery.ajax({
            type: 'POST',
            url: urlStr,
            data: postJson,
            success:function(data){
                if(data != "SUCCESS"){
                    banger.page.showMessageBox(data);
                    return false;
                }else{
                    //执行保存成功操作
                    saveLoan();
                }
            }
        });
    }else{
        //执行保存成功操作
        saveLoan();
    }
}

/**
 * 申请拒绝贷款
 *
 * @param loanId
 * @param loanStatusId
 */
function applyRefuse(loanId,loanStatusId){
    isRefuseLoan = true;
    var ret = applySave();
    var applyRemark = jQuery.trim($("#applyRemark").val());
    var radioValue = refuseApplyLoan();
    if(radioValue){
        radioValue['remark'] = applyRemark;
        radioValue['loanId'] = loanId;
        radioValue['beforeStatusId'] = loanStatusId;
        radioValue['customerId'] = $("#txtCustomerId").val();
        jQuery.ajax({
            type:'POST',
            url:'../loan/cancelLoan.html?RandNum='+Math.random(),
            data:radioValue,
            success:function(){
                tab.close(GetId(), true);
            }
        });
    }else{
        isRefuseLoan = false;
    }
}

//拒绝贷款
function refuseApplyLoan(){
    var url = "../loan/cancelLoanPage.html?RandNum="+Math.random();
    var result = window.showModalDialog(url,{window: window},'dialogWidth=550px;dialogHeight=500px;toolbar=no;menubar=no;scrollbars=no;resizable=no;location=no;status=no');
    return result;
}

