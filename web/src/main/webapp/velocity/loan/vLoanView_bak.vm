        function HashMap(){
            this.map = {};
        }
        HashMap.prototype = {
            put : function(key , value){
                this.map[key] = value;
            },
            get : function(key){
                if(this.map.hasOwnProperty(key)){
                    return this.map[key];
                }
                return null;
            },
            remove : function(key){
                if(this.map.hasOwnProperty(key)){
                    return delete this.map[key];
                }
                return false;
            },
            removeAll : function(){
                this.map = {};
            },
            keySet : function(){
                var _keys = [];
                for(var i in this.map){
                    _keys.push(i);
                }
                return _keys;
            }
        };
        HashMap.prototype.constructor = HashMap;

        var tdmap = new HashMap();
        function clickDeviationTD(td){
            var name = td.attr('name');
            var value = td.attr('value');

            //查找该列是否有选中
