            }
            if(null==lessenValue||""==lessenValue||''==lessenValue||undefined==lessenValue){
                lessenValue = "0";
            }
            if(null==rightsRealAmount||""==rightsRealAmount||''==rightsRealAmount||undefined==rightsRealAmount){
                rightsRealAmount = "0";
            }

            //应有权益 validationRights_rightsDueAmount
            var rightsDueAmount = parseFloat(rightsBeginAmount)+parseFloat(increaseAmount)+parseFloat(increaseFinancing)+parseFloat(increaseValue)-parseFloat(lessenAmount)-parseFloat(lessenValue);
