            //实际权益 validationRights_rightsRealAmount
//        var rightsRealAmount = parseFloat(rightsDueAmount)
//        $("#validationRights_rightsRealAmount").val(rightsRealAmount);

            //差别 validationRights_rightsDepreciationValue
            var rightsDepreciationValue = rightsDueAmount - rightsRealAmount;
            $("#validationRights_rightsDepreciationValue").val(rightsDepreciationValue);

            //偏差率 validationRights_rightsDepreciationRate
            if(rightsDueAmount!=0){
                $("#validationRights_rightsDepreciationRate").val(parseFloat(rightsDepreciationValue)/parseFloat(rightsDueAmount)*100);
            }else{
                $("#validationRights_rightsDepreciationRate").val(0);
            }

        }

        //格式化数字
        function fmoney(num,n)
        {

            var reg=/\,/;
            if(reg.test(num)){
                return num;
            }
            if(isNaN(num)||num==0||!isFinite(num)){
                return 0;
            }
            //参数说明：num 要格式化的数字 n 保留小数位
            num=Number(num)
            num = num.toFixed(n).toString();
            var re = /(-?\d+)(\d{3})/;
            while(re.test(num)) {
                num = num.replace(re,"$1,$2");
            }
            return num.substring(0,num.length-1);
        }
        //将数字还原
        function rmoney(num)
        {
