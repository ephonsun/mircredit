//          有选中判断是否和本次选中的相同,相同不处理（消除？） 不同则消除原来的 选中当前的
                var oldValue = tdmap.get(name);
                if(oldValue!=value){
                    tdmap.put(name,value);
                    td.css("background","#F2F2F2");
                    $("td[name='"+name+"']").each(function(){
                        var curValue = $(this).attr('value');
                        if(oldValue==curValue){
                            $(this).css("background","skyblue");
                        }
                    });
                }else{
                    //@TODO 再次点击 去除选中
                }
            }else{
                tdmap.put(name,value);
                td.css("background","#F2F2F2");
            }
        }



        function findPosX(obj) {
            var curleft = 0;

            if (obj.offsetParent) { //返回父类元素，大多说offsetParent返回body
                while (obj.offsetParent) {//遍历所有父类元素
                    curleft += obj.offsetLeft;//当前元素的左边距
                    obj = obj.offsetParent;
                }
            } else if (obj.x) curleft += obj.x;
            return curleft;
        }

        function findPosY(obj) {
            var curtop = 0;

            if (obj.offsetParent) {
                while (obj.offsetParent) {

                    curtop += obj.offsetTop;
                    obj = obj.offsetParent;
                }
            } else if (obj.y) curtop += obj.y;
            return curtop;
        }
