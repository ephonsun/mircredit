#parse("/velocity/taglibs.vm")<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><head>    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />    <title>营销计划执行情况统计表</title>   	#styleLink("base")	<link type="text/css" rel="stylesheet" href="${ctx}/css/public.css" />    <link type="text/css" rel="stylesheet" href="${ctx}/css/jquery.ui/base/jquery.ui.base.css" />	<link rel="stylesheet" href="${ctx}/css/tree/zTreeStyle.css" type="text/css">	<style type="text/css" media="print">         .Noprint{ display:none; }        .PageNext{ page-break-after:always; }		.NOPRINT { font-size:9pt; font-family:'宋体'; }        .tdp { border-bottom:1px solid #000; border-left:1px solid #000; border-right:1px solid #fff; border-top:1px solid #fff; }         .tabp { border-width:2px 2px 1px 1px; border-style: solid; border-color: #000 #000 #000 #000; }    </style>	#scriptLink("core")    #scriptLink("page")		<script type="text/javascript" src="${ctx}/js/system/b.base.js"></script>    <script type="text/javascript" src="${ctx}/js/plugins/jquery.plugins.js"></script>	<script type="text/javascript" src="${ctx}/js/plugins/jquery.validator-v1.0.js"></script>    <script type="text/javascript" src="${ctx}/js/jquery/jquery.ui/jquery.ui.core.js"></script>    <script type="text/javascript" src="${ctx}/js/jquery/jquery.ui/jquery.ui.widget.js"></script>    <script type="text/javascript" src="${ctx}/js/jquery/jquery.ui/jquery.ui.datepicker.js"></script>	<script type="text/javascript" src="${ctx}/js/plugins/jquery.ztree.core-3.2.js"></script>    <script type="text/javascript" src="${ctx}/js/plugins/jquery.ztree.excheck-3.2.js"></script>    <script type="text/javascript" src="${ctx}/js/banger/banger.tabs-v1.interface.js"></script>	<script type="text/javascript" src="${ctx}/js/report/LodopFuncs.js"></script>	<script type="text/javascript" src="${ctx}/js/system/b.dialog.js"></script>    <object id="LODOP_OB" classid="clsid:2105C259-1E0C-4534-8141-A753534CB4CA" width=0 height=0> 		<embed id="LODOP_EM" type="application/x-print-lodop" width=0 height=0 pluginspage="../../js/report/install_lodop32.exe"></embed>		<embed id="LODOP_EM" type="application/x-print-lodop" width=0 height=0 pluginspage="../../js/report/install_lodop64.exe"></embed>	</object> 	<script type="text/javascript">var oldJson;var containSub = '1';$(document).ready(function(){	$('select').select();	$('#form1').validator();	$("#query").click(function(){		queryReport();	});	$("#export").click(function(){		exportReport();	});	$("#print").click(function(){		openPrintPage();	});		$("#belongTo").change(function(){		changeBelongTo($(this).val());	});				//加载下属树	$('#userList').subtree({		tid: 'subtree',		showIcon: true,		url: function(){ return '/customer/getCusBelongToJson.html?BelongToType=brUser&random=' + Math.random(); },		buttons: [			{ text: '确定', callback: onCheckUser },			{ text: '取消' }		]	});		//加载机构树	$('#deptList').agetree({		tid: 'agetree',		url: function(){ return '/customer/getCusBelongToJson.html?BelongToType=brDept&random=' + Math.random(); },		buttons: [			{ text: '确定', callback: onCheckDept },			{ text: '取消' }		]	});		changeBelongTo($("#belongTo").val());	});//点击Userfunction onCheckUser(treeId){	var postJson = {};	postJson = getCheckJson(treeId, 'U');	$('#userIds').val(postJson.ids);	$('#userNames').val(postJson.names);	$("#userNames").focus().blur();}//点击Deptfunction onCheckDept(treeId,checkbox){	var postJson = {};	postJson = getCheckJson(treeId, 'D');	$('#deptIds').val(postJson.ids);	$('#deptNames').val(postJson.names);	if(checkbox.checked){		containSub = '1';	}else{		containSub = '0';	}	$("#deptNames").focus().blur();}function getCheckJson(treeId, flag){	var treeObj = jQuery.fn.zTree.getZTreeObj(treeId);	var nodes = treeObj.getCheckedNodes(true);	var len = nodes.length;	var uIds = "";	var uName = "";	for(var i=0; i<len; i++){		if(flag=='U'){			if(nodes[i].flag==flag){				if(uIds==""){					uIds = nodes[i].id;					uName = nodes[i].name;				}else{					uIds = uIds + "," + nodes[i].id;					uName = uName+ "," +nodes[i].name;				}			}		}else{			if(uIds==""){				uIds = nodes[i].id;				uName = nodes[i].name;			}else{				uIds = uIds + "," + nodes[i].id;				uName = uName+ "," +nodes[i].name;			}			}	}	var postJson = {};	postJson['ids'] = uIds;	postJson['names'] = uName;	return postJson;}var oldPostJson={};//查询报表function queryReport(){	var bool = jQuery.validator({ form: 'form1', extend: false});	if(!bool){		return;	}	var flag = checkCondition();	if(!flag)return;	var json = getConditionJson();	oldPostJson=json;	$('#reportBody').html('<div class=\'ui-loadbar\'><div class=\'icon\'>数据加载中，请耐心等待…</div></div>');	jQuery.post("/report/planReportQuery.html?random="+Math.random()*1000000,json,		function(html){		 refreshReport(html);	});}function getConditionJson(){	var json = getPostJson();	var belongTo = $("#belongTo").val();	switch(belongTo)	{		case "my":			json["userIds"]=$("#myUserId").val();			break;		case "sub":			json["userIds"]=$("#userIds").val();			break;		case "dept":			json["deptIds"]=$("#deptIds").val();			json["containSub"]=containSub;			break;	}	json["startDate"]=$("#datepicker1").val();	json["endDate"]=$("#datepicker2").val();	json["containSub"]=containSub;		//报表类型	var reportType = "";	$("input[name='radio']").each(function(){		if ($(this).attr("checked") == true) {			reportType = $(this).val();		}		});	json["reportType"]=reportType;		oldJson = json;	return json;}function refreshReport(html){	var elem = $(html);	var grid = $("#reportBody");	grid[0].innerHTML="";	grid.append(elem);}function changeBelongTo(val){	switch(val){		case "all":			$("#userList, #deptList,#my").hide();			break;		case "my":			$("#my").show();			$("#userList, #deptList").hide();			break;		case "sub":			$("#userList").show();			$("#my, #deptList").hide();			break;		case "dept":			$("#deptList").show();			$("#my, #userList").hide();			break;		default: break;	}}function exportReport(html){	if($("#reportBody").text()==""){		banger.page.showMessageBox("没有可导出的数据！");		return false;	}	var json = oldPostJson;	var params = "";	for(var nm in json)	{		params +="&"+nm+"="+json[nm];	}	location.href="/report/planReportExport.html?random="+Math.random()*1000000+params;}function CheckIsInstall() {		var bool = true;	try{	     var LODOP=getLodop(document.getElementById('LODOP_OB'),document.getElementById('LODOP_EM'));		if ((LODOP==null)||(typeof(LODOP.VERSION)=="undefined")||(typeof(LODOP.VERSION)=="unknown")) 			bool = false;	 }catch(err){		bool = false;	 }	 return bool;}function openPrintPage(){	var json = oldJson;	if(!json){		banger.page.showMessageBox("没有可打印的数据！");		return;	}	if(CheckIsInstall()){		prn1_preview();	}else{		//动态生成一个div 下载框		if (navigator.userAgent.indexOf('Win64')>=0){			path = '${ctx}/js/report/install_lodop64.exe';		}else{			path = '${ctx}/js/report/install_lodop32.exe';		}		loadPrintActiveX();	}} //打印预览支持控件路径var path = '';function loadPrintActiveX(){	var installHTML = '<div class=\'ui-model-install\'>'				+ '<h4 style=\'\'>请安装安全报表打印控件，已保证打印预览正常使用！</h4>'				+ '<p style=\'\'><label>控件安装完成后，请重启浏览器！</label></p>'				+ '<div style=\'\'><a href=\'' + path + '\' target=\'_self\'>立即下载</a></div>'				+ '</div>';	art.dialog({		id: 'loadPrintActiveX',		title: '安全报表打印控件下载',		content: installHTML	});}function prn1_preview() {		CreateOneFormPage();		LODOP.PREVIEW();	};	function CreateOneFormPage(){	LODOP=getLodop(document.getElementById('LODOP_OB'),document.getElementById('LODOP_EM'));  	var strStyleCSS="#parse('/velocity/taglibs.vm')"		+ "<link type='text/css' rel='stylesheet' href='${ctx}/css/base.css' />"		+ "<link type='text/css' rel='stylesheet' href='${ctx}/css/common.css' />";	var strFormHtml=strStyleCSS+"<body>"+document.getElementById("reportBody").innerHTML+"</body>";			LODOP.PRINT_INIT("");	LODOP.SET_PRINT_PAGESIZE(2,0,0,"A4");	LODOP.ADD_PRINT_HTM(20,0,"100%","100%",strFormHtml);};//检查报表条件function checkCondition(){	return true;}function relaTask(sortNo) {	var executeUserId = $("#userId_"+sortNo).val();	var productTarget = encodeURI(encodeURI($("#product_"+sortNo).html()));	var startDate =  oldJson["startDate"];	var endDate =  oldJson["endDate"];	var url = "/tskMarketingPlan/relaReportMarketing.html?executeUserId="+executeUserId+"&productTarget="+productTarget+"&startDate="+startDate+"&endDate="+endDate+"&random="+Math.random();	var options = {id: "rela_marketing"+sortNo, pid: GetId(), title: "关联营销任务", url: url, lock: false,confirm: false};    tab.add(options);}function searchStartDateFun(obj){	var startDate=$("#datepicker1").val();	var endDate=$("#datepicker2").val();	if(endDate.length>0&&startDate.length>0){		if(endDate<startDate){			$(obj).attr('tips', '开始日期必须小于结束日期'); return false;		}else{			$('#endDiv').removeClass('v-fails');			return true;		}	}else return true;}function searchEndDateFun(obj){	var startDate=$("#datepicker1").val();	var endDate=$("#datepicker2").val();	if(endDate.length>0&&startDate.length>0){		if(startDate>endDate){			$(obj).attr('tips', '结束日期不能小于开始日期'); return false;		}else{			$('#startDiv').removeClass('v-fails');			return true;		}	}else return true;}function executeUserFunction(obj){	var belongTo=$("#belongTo").val();	if(belongTo=="sub"){		var text=$("#userNames").val();		if(text.length==0){			$(obj).attr('tips', '“统计对象下属”必须填写'); return false;		}	}	return true;}function executeUserDeptFunction(obj){	var belongTo=$("#belongTo").val();	if(belongTo=="dept"){		var text=$("#deptNames").val();		if(text.length==0){			$(obj).attr('tips', '“统计对象机构”必须填写'); return false;		}	}	return true;}</script></head><body style="padding-bottom:100px;">	<form id="form1" method="post">		<div class="condi-row">			<div class="condi-i">				<label class="condi-i-field">统计对象：</label>				<select id="belongTo" name="" render="sel" style="width: 120px;">					#if($isManage)					<option value="all"></option>					#end                    <option value="my">我的</option>                    #if($isManage)                    	<option value="sub">下属的</option>                    	<option value="dept">机构的</option>                    #end                </select>                <span id="my" class="btn2 fl" style="display: none; margin-left: 5px;">                	<input id="myUserId" type="hidden" value="$!userId" />                    <label class="btn2-txt">$!userName</label>                </span>                <div id="userList" class="dtxt2 fl" style="display: none; margin-left: 5px; width: 200px;">                    <input type="text" fun="executeUserFunction" id="userNames" class="dtxt2-txt" readonly="readonly" />                    <input id="userIds" type="hidden" />                    <span class="dtxt2-ddl">ddl</span>                </div>                <div id="deptList" class="dtxt2 fl" style="display: none; margin-left: 5px; width: 200px;">                    <input type="text" fun="executeUserDeptFunction" id="deptNames" class="dtxt2-txt" readonly="readonly" />                    <input id="deptIds" type="hidden" />                    <span class="dtxt2-ddl">ddl</span>                </div>            </div>        </div>		<div class="condi-row">			<div class="condi-i">				<label class="condi-i-field">统计时间：</label>				<div class="dtxt2 floatL" id="startDiv" style="width: 120px;">                   <input type="text" fun="searchStartDateFun" readonly="readonly" id="datepicker1" class="dtxt2-txt" />                   <span class="dtxt2-date">date</span>                </div>				<label class="dash">—</label>                <div class="dtxt2 floatL" id="endDiv" style="width: 120px;">                    <input type="text" fun="searchEndDateFun" readonly="readonly" id="datepicker2" class="dtxt2-txt"/>                    <span class="dtxt2-date">date</span>                </div>			</div>        </div>		</form>	 <div class="sz">		<input id="query" type="button" class="btn1" value="搜索" />		#if($urlPermitUtil.hasPermission('exportAddCustomerReport'))		<input id="export" type="button" class="btn1" value="导出" />		#end		#if($urlPermitUtil.hasPermission('getAddCustomerReportPrint'))		<input id="print" type="button" class="btn1" value="打印" />		#end	</div>	 <div class="recenter" id="reportBody">	</div>	<div id="downLoad"  style="display:none"><a href="../../js/report/install_lodop32.exe" target="_self">下载</a></div></body></html>