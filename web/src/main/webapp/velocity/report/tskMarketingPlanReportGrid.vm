<h4 class="recenter-ttl">	<label>	#set($startDate = $date.format("yyyy-MM-dd",$!startDate))	#set($endDate = $date.format("yyyy-MM-dd",$!endDate))	#if($!startDate!=""&&$!endDate!="")		$!startDate至$!endDate	#elseif($!startDate!=""&&$!endDate=="")		$!startDate之后	#elseif($!startDate==""&&$!endDate!="")		截止至$!endDate	#end	营销计划执行情况统计表	</label></h4><table class="recenter-tbl">	<thead>		<tr>			<th><label title="执行者">执行者</label></th>			<th><label title="产品指标">产品指标</label></th>			<th><label title="营销目标(万元)">营销目标(万元)</label></th>			<th><label title="计划营销额(万元)">计划营销额(万元)</label></th>			<th><label title="实际营销额(万元)">实际营销额(万元)</label></th>			<th><label title="实际占计划比">实际占计划比</label></th>			<th><label title="实际关联任务">实际关联任务</label></th>			<th><label title="排名">排名</label></th>		</tr>	</thead>	<tbody>		#foreach($data in $!dataList)		<tr>			<td>				<label title="$!data.executeUserName">$!data.executeUserName</label>				<input type="hidden" id="userId_$!data.sortNo" value="$!data.executeUserId"/> 			</td>			<td><label title="$!data.productTarget" id="product_$!data.sortNo">$!data.productTarget</label></td>			<td><label title="$vmHelper.getDecimalWanMoney($!data.marketingTarget)">$vmHelper.getDecimalWanMoney($!data.marketingTarget)</label></td>			<td><label title="$!data.executeUserName">$vmHelper.getDecimalWanMoney($!data.planTarget)</label></td>			<td><label title="$!data.executeUserName">$vmHelper.getDecimalWanMoney($!data.saleMoney)</label></td>			<td><label title="$vmHelper.getPercentDec($!data.percent)">$vmHelper.getPercentDec($!data.percent)</label></td>			<td>				#if($!data.taskCount==0)0#else				<label title="$!data.taskCount" onclick="relaTask($!data.sortNo)" class="ui-link-btn">$!data.taskCount</label>				#end			</td>			<td><label title="$!data.sortNo">$!data.sortNo</label></td>		</tr>		#end	</tbody></table><p class="recenter-footnote" style="text-align: right;">	制表人：$!userName	&nbsp; &nbsp; 	制表时间：$!reportDate</p>