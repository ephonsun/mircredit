<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="sysTalklimitConfig" extends="default" namespace="/sysTalklimitConfig" >
		 <!--显示有效通话列表页面-->
		<action name="showTalklimitList" class="sysTalklimitConfigAction" method="showTalklimitList">
			<result name="success">/velocity/system/sysTalklimitConfigList.vm</result>
			<result name="error">/velocity/error.vm</result>
		</action>
		
		
		<!--保存数据 -->
		<action name="saveTalklimitConfigList" class="sysTalklimitConfigAction" method="saveTalklimitConfigList">
			
		</action>
		
		<!--开关设置 -->
		<action name="setTalklimitConfig" class="sysTalklimitConfigAction" method="setTalklimitConfig">
		</action>
		<!--获取开关设置-->
		<action name="getTalklimitConfigSwitch" class="sysTalklimitConfigAction" method="getTalklimitConfigSwitch">
		</action>
		
	</package>

</struts>