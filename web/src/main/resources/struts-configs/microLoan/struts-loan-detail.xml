<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="vLoan" extends="default" namespace="/loan">
        <!-- 申请贷款 -->
        <action name="vLoan" class="viewLoanDetailAction" method="vLoan">
            <result name="success">/velocity/loan/vLoan.vm</result>
            <result name="error">/velocity/error.vm</result>
        </action>
        <action name="vLoanView" class="viewLoanDetailAction" method="vLoanView">
            <result name="success">/velocity/loan/vLoanView.vm</result>
            <result name="error">/velocity/error.vm</result>
        </action>
        <action name="flashDataManagerView6" class="viewLoanDetailAction" method="flashDataManagerView">
            <result name="success">/velocity/loan/makeLoanDataTableView.vm</result>
            <result name="error">/velocity/error.vm</result>
        </action>
        <action name="flashDataManagerView1" class="viewLoanDetailAction" method="flashDataManagerView">
            <result name="success">/velocity/loan/askLoanDataTableView.vm</result>
            <result name="error">/velocity/error.vm</result>
        </action>
        <action name="flashDataManagerView3" class="viewLoanDetailAction" method="flashDataManagerView">
            <result name="success">/velocity/loan/unExamLoanDataTableView.vm</result>
            <result name="error">/velocity/error.vm</result>
        </action>
        <action name="flashDataManagerView4" class="viewLoanDetailAction" method="flashDataManagerView">
            <result name="success">/velocity/loan/approveLoanDataTableView.vm</result>
            <result name="error">/velocity/error.vm</result>
        </action>
        <action name="flashDataManagerView5" class="viewLoanDetailAction" method="flashDataManagerView">
            <result name="success">/velocity/loan/lendLoanDataTableView.vm</result>
            <result name="error">/velocity/error.vm</result>
        </action>
        <action name="flashDataManagerView7" class="viewLoanDetailAction" method="flashDataManagerView">
            <result name="success">/velocity/loan/dunLnLoanDataTableView.vm</result>
            <result name="error">/velocity/error.vm</result>
        </action>
        <action name="openCusDataListTableView" class="viewLoanDetailAction" method="openCusDataListTableView">
            <result name="success">/velocity/loan/cusDataListTable.vm</result>
            <result name="error">/velocity/error.vm</result>
        </action>
	</package>
</struts>