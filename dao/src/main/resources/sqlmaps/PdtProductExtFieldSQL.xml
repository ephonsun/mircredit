	<!-- 新增产品自定义  -->
	<insert id="addProductExtField" parameterClass="java.util.Map">
		insert into PDT_PRODUCT_EXT_FIELD
		(
		 EXT_FIELD_ID,
		 PRODUCT_ID
		 <isPropertyAvailable  property="fieldstr">
		 	<isNotEmpty prepend="," property="fieldstr">
				$fieldstr$
			</isNotEmpty>
		 </isPropertyAvailable>
		 )
		values
		(
		SEQ_PDT_PRODUCT_EXT_FIELD.NEXTVAL,
		#productId#
		<isPropertyAvailable  property="valuestr">
		 	<isNotEmpty prepend="," property="valuestr">
				$valuestr$
			</isNotEmpty>
		 </isPropertyAvailable>
		)
	</insert>
	
	<!-- 修改产品自定义  -->
	<update id="updateProductExtField" parameterClass="java.util.Map">
		UPDATE PDT_PRODUCT_EXT_FIELD
	      SET
	      $updatestr$
	      WHERE
	      PRODUCT_ID = #productId#
	</update>
	
	<!-- 删除产品自定义  -->
   <delete id="deleteProductExtField" parameterClass="java.lang.Integer">
	<![CDATA[
		delete from PDT_PRODUCT_EXT_FIELD r where r.EXT_FIELD_ID=#Id#
	]]>
   </delete>
   <!-- 根据产品id查询实体  -->
   <select id="getProductExtFieldById" resultMap="PdtProductExtFieldResult" parameterClass="java.lang.Integer">
	<![CDATA[
		select 
                   *
             from 
                   PDT_PRODUCT_EXT_FIELD a
                   where 
                  	a.PRODUCT_ID=#Id# 
	]]>
