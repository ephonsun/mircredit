	<!-- 新增客户自定义  -->
	<insert id="addCustomizedField" parameterClass="java.util.Map">
		insert into CRM_CUSTOMER_EXT_FIELD
		(
		 EXT_FIELD_ID,
		 CUSTOMER_ID
		 <isPropertyAvailable  property="fieldstr">
		 	<isNotEmpty prepend="," property="fieldstr">
				$fieldstr$
			</isNotEmpty>
		 </isPropertyAvailable>
		 )
		values
		(
		SEQ_CRM_CUSTOMER_EXT_FIELD.NEXTVAL,
		#customerId#
		<isPropertyAvailable  property="valuestr">
		 	<isNotEmpty prepend="," property="valuestr">
				$valuestr$
			</isNotEmpty>
		 </isPropertyAvailable>
		)
	</insert>

